<?xml version="1.0" encoding="ASCII"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="editormodel.ecore#//Game/stories"/>
    <ownedChild
        relatedDiagrams="//@diagram">
      <domainMetaElement
          href="editormodel.ecore#//Story"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="editormodel.gmfgraph#StoryLabel"/>
        <features
            href="editormodel.ecore#//Story/description"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="editormodel.gmftool#//@palette/@tools.0/@tools.4"/>
      <diagramNode
          href="editormodel.gmfgraph#Story"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="editormodel.ecore#//Story/subStories"/>
        <ownedChild
            relatedDiagrams="//@diagram">
          <domainMetaElement
              href="editormodel.ecore#//SubStory"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="editormodel.gmfgraph#SubStoryLabel"/>
            <features
                href="editormodel.ecore#//SubStory/description"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="editormodel.gmftool#//@palette/@tools.0/@tools.5"/>
          <diagramNode
              href="editormodel.gmfgraph#SubStory"/>
          <children
              compartment="//@nodes.0/@ownedChild/@children.0/@ownedChild/@compartments.0">
            <containmentFeature
                href="editormodel.ecore#//SubStory/levels"/>
            <ownedChild
                relatedDiagrams="//@diagram">
              <domainMetaElement
                  href="editormodel.ecore#//Level"/>
              <labelMappings
                  xsi:type="gmfmap:FeatureLabelMapping">
                <diagramLabel
                    href="editormodel.gmfgraph#LevelLabel"/>
                <features
                    href="editormodel.ecore#//Level/description"/>
              </labelMappings>
              <tool
                  xsi:type="gmftool:CreationTool"
                  href="editormodel.gmftool#//@palette/@tools.0/@tools.1"/>
              <diagramNode
                  href="editormodel.gmfgraph#Level"/>
              <children
                  compartment="//@nodes.0/@ownedChild/@children.0/@ownedChild/@children.0/@ownedChild/@compartments.0">
                <containmentFeature
                    href="editormodel.ecore#//Level/objectives"/>
                <ownedChild>
                  <domainMetaElement
                      href="editormodel.ecore#//Objective"/>
                  <labelMappings
                      xsi:type="gmfmap:FeatureLabelMapping">
                    <diagramLabel
                        href="editormodel.gmfgraph#ObjectiveLabel"/>
                    <features
                        href="editormodel.ecore#//Objective/description"/>
                  </labelMappings>
                  <tool
                      xsi:type="gmftool:CreationTool"
                      href="editormodel.gmftool#//@palette/@tools.0/@tools.3"/>
                  <diagramNode
                      href="editormodel.gmfgraph#Objective"/>
                </ownedChild>
              </children>
              <children
                  compartment="//@nodes.0/@ownedChild/@children.0/@ownedChild/@children.0/@ownedChild/@compartments.1">
                <containmentFeature
                    href="editormodel.ecore#//Level/levelCase"/>
                <ownedChild
                    relatedDiagrams="//@diagram">
                  <domainMetaElement
                      href="editormodel.ecore#//LevelCase"/>
                  <labelMappings
                      xsi:type="gmfmap:FeatureLabelMapping">
                    <diagramLabel
                        href="editormodel.gmfgraph#LevelCaseLabel"/>
                    <features
                        href="editormodel.ecore#//LevelCase/description"/>
                  </labelMappings>
                  <tool
                      xsi:type="gmftool:CreationTool"
                      href="editormodel.gmftool#//@palette/@tools.0/@tools.2"/>
                  <diagramNode
                      href="editormodel.gmfgraph#LevelCase"/>
                  <children
                      compartment="//@nodes.0/@ownedChild/@children.0/@ownedChild/@children.0/@ownedChild/@children.1/@ownedChild/@compartments.0">
                    <containmentFeature
                        href="editormodel.ecore#//LevelCase/draggableItems"/>
                    <ownedChild>
                      <domainMetaElement
                          href="editormodel.ecore#//DraggableItem"/>
                      <labelMappings
                          xsi:type="gmfmap:FeatureLabelMapping">
                        <diagramLabel
                            href="editormodel.gmfgraph#DraggableItemLabel"/>
                        <features
                            href="editormodel.ecore#//DraggableItem/description"/>
                      </labelMappings>
                      <tool
                          xsi:type="gmftool:CreationTool"
                          href="editormodel.gmftool#//@palette/@tools.0/@tools.0"/>
                      <diagramNode
                          href="editormodel.gmfgraph#DraggableItem"/>
                    </ownedChild>
                  </children>
                  <compartments
                      children="//@nodes.0/@ownedChild/@children.0/@ownedChild/@children.0/@ownedChild/@children.1/@ownedChild/@children.0">
                    <compartment
                        href="editormodel.gmfgraph#LevelCaseDraggableItemsCompartment"/>
                  </compartments>
                </ownedChild>
              </children>
              <compartments
                  children="//@nodes.0/@ownedChild/@children.0/@ownedChild/@children.0/@ownedChild/@children.0">
                <compartment
                    href="editormodel.gmfgraph#LevelObjectivesCompartment"/>
              </compartments>
              <compartments
                  children="//@nodes.0/@ownedChild/@children.0/@ownedChild/@children.0/@ownedChild/@children.1">
                <compartment
                    href="editormodel.gmfgraph#LevelLevelCaseCompartment"/>
              </compartments>
            </ownedChild>
          </children>
          <compartments
              children="//@nodes.0/@ownedChild/@children.0/@ownedChild/@children.0">
            <compartment
                href="editormodel.gmfgraph#SubStoryLevelsCompartment"/>
          </compartments>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0">
        <compartment
            href="editormodel.gmfgraph#StorySubStoriesCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="editormodel.ecore#//Path"/>
    <containmentFeature
        href="editormodel.ecore#//Level/path"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="editormodel.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="editormodel.gmfgraph#Path"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="editormodel.ecore#//Path/prevLevel"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="editormodel.ecore#//Path/nextLevel"/>
  </links>
  <diagram>
    <diagramCanvas
        href="editormodel.gmfgraph#editormodel"/>
    <domainModel
        href="editormodel.ecore#/"/>
    <domainMetaElement
        href="editormodel.ecore#//Game"/>
    <palette
        href="editormodel.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
