context ObjectModel!ObjectModel {
    
  // At least there is an object with name button
  constraint AnLeastOneObjectExistWithNameButton {
    check : 
        self.checkThereIsAnObjectNamedButton()
    message : 
        "At least one object named 'button' must exist"
  }
} 

operation ObjectModel checkThereIsAnObjectNamedButton(): Boolean{
    if (self.objects.size() > 0){  
	    for (object in self.objects){
	        if (object.name = "textbox"){
	        //"Model contains an object with name 'textbox': TRUE".println();
	        return true;
	        }
	    }
    }
    //"Model contains an object with name 'textbox': FALSE".println();
    return false;
}