<?xml version="1.0" encoding="ASCII"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="GamifiedModelling.ecore#//Game/levels"/>
    <ownedChild
        relatedDiagrams="//@diagram">
      <domainMetaElement
          href="GamifiedModelling.ecore#//Level"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="GamifiedModelling.gmfgraph#LevelLabel"/>
        <features
            href="GamifiedModelling.ecore#//Level/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="GamifiedModelling.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="GamifiedModelling.gmfgraph#Level"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="GamifiedModelling.ecore#//Level/objectives"/>
        <ownedChild>
          <domainMetaElement
              href="GamifiedModelling.ecore#//Objective"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="GamifiedModelling.gmfgraph#ObjectiveLabel"/>
            <features
                href="GamifiedModelling.ecore#//Objective/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="GamifiedModelling.gmftool#//@palette/@tools.0/@tools.2"/>
          <diagramNode
              href="GamifiedModelling.gmfgraph#Objective"/>
        </ownedChild>
      </children>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.1">
        <containmentFeature
            href="GamifiedModelling.ecore#//Level/case"/>
        <ownedChild>
          <domainMetaElement
              href="GamifiedModelling.ecore#//Case"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="GamifiedModelling.gmfgraph#CaseLabel"/>
            <features
                href="GamifiedModelling.ecore#//Case/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="GamifiedModelling.gmftool#//@palette/@tools.0/@tools.0"/>
          <diagramNode
              href="GamifiedModelling.gmfgraph#Case"/>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0">
        <compartment
            href="GamifiedModelling.gmfgraph#LevelObjectivesCompartment"/>
      </compartments>
      <compartments
          children="//@nodes.0/@ownedChild/@children.1">
        <compartment
            href="GamifiedModelling.gmfgraph#LevelCaseCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="GamifiedModelling.ecore#//Path"/>
    <containmentFeature
        href="GamifiedModelling.ecore#//Level/path"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="GamifiedModelling.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="GamifiedModelling.gmfgraph#Path"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="GamifiedModelling.ecore#//Path/prevLevel"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="GamifiedModelling.ecore#//Path/nextLevel"/>
  </links>
  <diagram>
    <diagramCanvas
        href="GamifiedModelling.gmfgraph#gamifiedmodelling"/>
    <domainModel
        href="GamifiedModelling.ecore#/"/>
    <domainMetaElement
        href="GamifiedModelling.ecore#//Game"/>
    <palette
        href="GamifiedModelling.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
