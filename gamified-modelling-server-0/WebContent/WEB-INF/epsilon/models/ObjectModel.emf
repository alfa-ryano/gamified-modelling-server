@namespace(uri="org.york.gamified.modelling.objectmodel",prefix="org.york.gamified.modelling.objectmodel")

package objectmodel;

@gmf.diagram(foo="bar")
class ObjectModel {
    val Object[*] objects;
    val Link[*] links;
}

@gmf.node(label="name")
class Object {
    attr String name;
    attr String identity;
}

@gmf.link(source="fromObject", target="toObject")
class Link {
    attr String identity; 
    ref Object fromObject;
    ref Object toObject;
}


